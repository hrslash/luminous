{
    "name": "hrslash/luminous",
    "type": "wordpress-theme",
    "description": "The WordPress theme framework based on Laravel Lumen.",
    "keywords": ["luminous", "framework", "theme", "WordPress", "laravel", "lumen"],
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Takeru Hirose",
            "email": "info@hrslash.com",
            "homepage": "http://hrslash.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "composer/installers": "~1.0",
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Luminous\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
