{
    "name": "hrslash/luminous",
    "type": "wordpress-theme",
    "description": "[under development] The WordPress theme framework based on Laravel components.",
    "keywords": ["luminous", "framework", "theme", "WordPress", "laravel"],
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Takeru Hirose",
            "email": "takeru0757@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "composer/installers": "~1.0",
        "illuminate/bus": "5.1.*",
        "illuminate/config": "5.1.*",
        "illuminate/container": "5.1.*",
        "illuminate/contracts": "5.1.*",
        "illuminate/cache": "5.1.*",
        "illuminate/cookie": "5.1.*",
        "illuminate/encryption": "5.1.*",
        "illuminate/http": "5.1.*",
        "illuminate/mail": "5.1.*",
        "illuminate/pagination": "5.1.*",
        "illuminate/session": "5.1.*",
        "illuminate/support": "5.1.*",
        "illuminate/translation": "5.1.*",
        "illuminate/validation": "5.1.*",
        "illuminate/view": "5.1.*",
        "monolog/monolog": "~1.0",
        "nikic/fast-route": "0.6.*",
        "symfony/http-kernel": "2.7.*",
        "symfony/http-foundation": "2.7.*",
        "symfony/security-core": "2.7.*",
        "symfony/var-dumper": "2.7.*",
        "illuminate/filesystem": "^5.1",
        "illuminate/events": "^5.1"
    },
    "suggest": {
        "vlucas/phpdotenv": "Required to use .env files (~1.0)."
    },
    "autoload": {
        "psr-4": {
            "Luminous\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
